@model Mamodel
@{
    ViewBag.Ref = "template";
    ViewBag.Title = "模板";
}

<div class="text-center">
    <form method="post" class="container" action="AddtravelInfo">
        <h4>旅行人ID:</h4>
        <input name="ID" type="text" class="form-control" value="@Model.maID" readonly />
        <h4>旅行地点:</h4>
        <input name="site" type="text" class="form-control" placeholder = "旅行地点">
        <input type="submit" class="btn btn-primary" onclick="ifDate()" value="添加" />
        <a class="btn btn-secondary" href="/Travel/index">返回</a>
    </form>
</div>
