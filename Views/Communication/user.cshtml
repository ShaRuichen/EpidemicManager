@model UserCommunitionMobel
@{ ViewBag.Ref = "communication";
    ViewBag.Title = "用户问询"; }

<form class="Communication_form" method="post" action="user">


    <div class="form-group">
        <label for="user_idNumber" class="col-sm-2 control-label">用户身份证号</label>
        <div class="col-sm-10">
            <input type="text" id="user_id" />
        </div>
    </div>
    <div class="form-group">
        <label for="question_content" class="col-sm-2 control-label">问询具体内容</label>
        <div class="col-sm-10">
            <input type="text" id="question_content" />
        </div>
    </div>

    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <button type="submit" class="btn btn-light" onclick="click1()">提交</button>
        </div>
    </div>
</form>