@model DistributeModel
@{
    ViewBag.Ref = "Donation";
    ViewBag.Title = "分配";
}
<div class="text-center">
    <h3>分配信息</h3>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>捐献编号</th>
                <th>医院名称</th>
                <th>分配日期</th>
                <th>分配时间</th>
                <th>管理员编号</th>
            </tr>
        </thead>
        <tbody>
            @for(int i=0;i<Model.num;i++)
            {
            <tr>
                <td>@Model.DIds[i]</td>
                <td>@Model.Names[i]</td>
                <td>@Model.Dates[i]</td>
                <td>@Model.Times[i]</td>
                <td>@Model.MIds[i]</td>
            </tr>
            }

        </tbody>
    </table>
</div>
