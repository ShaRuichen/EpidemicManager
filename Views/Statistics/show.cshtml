@model StatisticsModel
@{
    ViewBag.Ref = "show";
    ViewBag.Title = "展示";
}

@{ int n = 0;}
<div class="text-center">
    <h1>@ViewBag.Huhaha</h1>
    <table>
        @for (int i = 0; i < Model.ID; i++)
        {
            <tr>
                <td>@Model.ID&emsp;</td>
                <td>@Model.Date&emsp;</td>
                <td>@Model.Time&emsp;</td>
                <td>@Model.Site&emsp;</td>

            </tr>
        }
    </table>
</div>